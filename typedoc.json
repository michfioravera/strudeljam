{
  "$schema": "https://typedoc.org/schema.json",
  "skipErrorChecking": true,
  "entryPoints": ["src"],
  "entryPointStrategy": "expand",
  "out": "public/docs",
  {
    "plugin": ["./typedoc-plugin.cjs", "./typedoc-plugin-italian.cjs"],
    "lang": "it"
  },
  "exclude": [
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/node_modules/**"
  ],
  "excludePrivate": true,
  "excludeInternal": true,
  "readme": "README.md",
  "tsconfig": "./tsconfig.json",
  "name": "ꜱᴛʀᴜᴅᴇʟᴊᴀᴍ",
  "includeVersion": true
}
